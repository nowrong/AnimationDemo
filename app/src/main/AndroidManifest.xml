<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.pkqup.androidnote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.android.pkqup.androidnote.abase.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity

            android:name="com.android.pkqup.androidnote.animation_test.FrameActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.android.pkqup.androidnote.animation_test.TweenActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.android.pkqup.androidnote.animation_test.PropertyActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.android.pkqup.androidnote.animation_test.PhysicsBasedAnimationActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.android.pkqup.androidnote.fragment_test.FragmentTestActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.android.pkqup.androidnote.handler_test.HandlerThreadTestActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.android.pkqup.androidnote.touch_event_test.TouchTestActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.android.pkqup.androidnote.view_draw_test.ViewDrawTestActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>